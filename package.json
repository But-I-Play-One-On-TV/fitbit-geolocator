{
	"name": "geolocator",
	"version": "0.1.0",
	"private": true,
	"license": "UNLICENSED",
	"devDependencies": {
		"@fitbit/sdk": "3.0.0",
		"@fitbit/sdk-cli": "1.5.0",
		"fitbit-sdk-types": "3.2.2",
		"prettier": "^1.16.4",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.17.0",
