{
	"name": "geolocator",
	"version": "1.0.0",
	"private": true,
	"license": "UNLICENSED",
	"devDependencies": {
		"@fitbit/sdk": "3.0.0",
		"@fitbit/sdk-cli": "1.5.0",
		"fitbit-sdk-types": "3.2.11",
		"prettier": "1.16.4",
		"tslint": "5.13.1",
		"tslint-config-prettier": "1.18.0",
		"typescript": "3.3.3333"
	},
	"fitbit": {
